{
    "name": "Facebook Relike",
    "description": "Relike for Facebook",
    "version": "0.1",
    "icons": { "48": "icon48.png",
              "128": "icon128.png" },
    "manifest_version": 2,
    "permissions": [
        "tabs", "http://www.facebook.com/*"
    ],
    "content_scripts": [
        {
            "matches": ["http://www.facebook.com/*"],
            "js": ["jquery-1.8.3.min.js", "jquery.xdomainajax.js", "facebook.js"]
        }
    ]
}
